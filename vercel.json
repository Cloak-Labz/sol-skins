{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "npm run build --prefix src/client",
  "devCommand": "npm run dev --prefix src/client",
  "installCommand": "npm install --prefix src/client",
  "framework": "nextjs",
  "outputDirectory": "src/client/.next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./src/client"
}
