{
  "name": "@sol-skins/server",
  "version": "1.0.0",
  "description": "SolSkins Backend API Server",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "tsx watch src/index.ts",
    "start": "node dist/index.js",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "typeorm-ts-node-commonjs migration:generate -d src/database/config.ts",
    "migration:run": "typeorm-ts-node-commonjs migration:run -d src/database/config.ts",
    "migration:revert": "typeorm-ts-node-commonjs migration:revert -d src/database/config.ts",
    "schema:sync": "typeorm-ts-node-commonjs schema:sync -d src/database/config.ts",
    "schema:drop": "typeorm-ts-node-commonjs schema:drop -d src/database/config.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "typeorm": "^0.3.17",
    "pg": "^8.11.3",
    "redis": "^4.6.10",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.1.5",
    "jsonwebtoken": "^9.0.2",
    "bcryptjs": "^2.4.3",
    "joi": "^17.11.0",
    "winston": "^3.11.0",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "dotenv": "^16.3.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.0",
    "express-validator": "^7.0.1",
    "express-async-errors": "^3.1.1",
    "@solana/web3.js": "^1.87.6",
    "tweetnacl": "^1.0.3",
    "bs58": "^5.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.9.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/morgan": "^1.9.9",
    "@types/uuid": "^9.0.7",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-express": "^4.1.6",
    "typescript": "^5.2.2",
    "tsx": "^4.1.2",
    "ts-node": "^10.9.1"
  },
  "keywords": [
    "solana",
    "nft",
    "csgo",
    "skins",
    "lootbox",
    "api"
  ],
  "author": "SolSkins Team",
  "license": "MIT"
} 