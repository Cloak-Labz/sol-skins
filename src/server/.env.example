# Admin
ADMIN_WALLET_PRIVATE_KEY=[12,13]
ADMIN_WALLETS=wallet2,wallet2
IRYS_PRIVATE_KEY=your-base58-encoded-private-key-here

# Server Configuration
NODE_ENV=development
PORT=4000
API_PREFIX=/api/v1

# Database Configuration
DB_HOST=localhost
DB_PORT=5433
DB_USERNAME=db_user
DB_PASSWORD=db_pass
DB_DATABASE=database
DB_SYNCHRONIZE=false
DB_LOGGING=true

# JWT Configuration
JWT_SECRET=your-super-secret-jwt-key-change-this-in-production
JWT_EXPIRE=24h
JWT_REFRESH_EXPIRE=7d

# Security
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Solana Configuration
SOLANA_RPC_URL=https://api.devnet.solana.com
PROGRAM_ID=program_id
BUYBACK_PROGRAM_ID=buyback_program_id

# Monitoring
LOG_LEVEL=info

# Discord
DISCORD_BOT_TOKEN=12354
DISCORD_TICKET_CHANNEL_ID=12345
DISCORD_GUILD_ID=12345
